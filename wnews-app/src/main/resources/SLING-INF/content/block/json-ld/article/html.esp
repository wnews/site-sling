<script type="application/ld+json">
{
  "@context": "http://schema.org",
  "@type": "NewsArticle",
  "headline": "<%= currentNode.title %>",
  "datePublished": "<%= currentNode.date %>"
<% if (currentNode.media) { %>,
  "image": {
    "@type": "ImageObject",
    "url": "http://wnews.org.au/wp/media/<%= currentNode.media %>"
  }
<% } %>
}
</script>
